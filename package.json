{
  "name": "biottic-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0 <23.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.x",
  "scripts": {
    "dev": "vite",
    "server": "tsx watch src/server/index.ts",
    "dev:all": "concurrently \"pnpm run dev\" \"pnpm run server\"",
    "build": "pnpm run build:all",
    "build:server": "tsc -p tsconfig.node.json",
    "build:client": "tsc && vite build",
    "build:all": "pnpm run build:server && pnpm run build:client",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "test:all": "pnpm typecheck && pnpm lint && pnpm test && pnpm test:coverage",
    "test:e2e:ci": "start-server-and-test dev:all http://localhost:5174 'cypress run --config-file cypress.ci.config.ts'",
    "test:e2e": "start-server-and-test dev:all http://localhost:5174 'cypress run'",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "verify": "pnpm test:all && pnpm test:e2e",
    "test:debug": "vitest --inspect-brk",
    "test:ui": "vitest --ui",
    "verify:step": "echo '=== Running Type Check ===' && pnpm typecheck && echo '=== Running Lint ===' && pnpm lint && echo '=== Running Unit Tests ===' && pnpm test && echo '=== Running E2E Tests ===' && pnpm test:e2e && echo '=== Running Build ===' && pnpm run build"
  },
  "dependencies": {
    "@stripe/stripe-js": "^3.0.7",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mongoose": "^5.11.97",
    "@types/nodemailer": "^6.4.14",
    "@types/react-helmet-async": "^1.0.3",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "joi": "^17.12.1",
    "lucide-react": "^0.344.0",
    "mongoose": "^8.1.1",
    "nodemailer": "^6.9.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.22.0",
    "tsx": "^4.7.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.0",
    "@eslint/js": "^8.57.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/testing-library__user-event": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^3.0.7",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cypress": "^13.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^15.0.0",
    "jsdom": "^24.0.0",
    "msw": "^2.0.0",
    "postcss": "^8.4.33",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.5.0",
    "typescript-eslint": "^7.0.0",
    "vite": "^5.4.14",
    "vitest": "^1.0.0"
  }
}
