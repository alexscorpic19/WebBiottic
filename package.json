{
  "name": "biottic-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "cross-env NODE_ENV=development tsx watch --tsconfig tsconfig.server.json src/server/index.ts",
    "dev:all": "concurrently \"pnpm run dev\" \"pnpm run server\"",
    "build": "pnpm run build:client && pnpm run build:server",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage && pnpm run test:e2e:ci",
    "test:e2e:ci": "start-server-and-test dev:all http://localhost:5173 'cypress run'"
  },
  "dependencies": {
    "@stripe/stripe-js": "^3.0.7",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/mongoose": "^5.11.97",
    "@types/nodemailer": "^6.4.17",
    "@types/react-helmet-async": "^1.0.3",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "joi": "^17.13.3",
    "lucide-react": "^0.344.0",
    "mongoose": "^8.10.0",
    "nodemailer": "^6.10.0",
    "pnpm": "^10.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.22.3",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.17.23",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "3.0.7",
    "@vitest/ui": "3.0.7",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.6",
    "eslint": "^9.9.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.35",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.3",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^6.2.1",
    "vitest": "^3.0.7"
  }
}
